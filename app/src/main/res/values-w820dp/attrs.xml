<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="HnGridLayout">
        <!--列数-->
        <attr name="columnsSize" format="integer"/>
        <!--水平间距-->
        <attr name="horizontalSpacing" format="dimension"/>
        <!--竖直间距-->
        <attr name="verticalSpacing" format="dimension"/>
        <!--首次是否折叠-->
        <attr name="firstCollapse" format="boolean"/>
        <!--折叠动画的时间-->
        <attr name="collapseDuration" format="integer"/>
    </declare-styleable>
</resources>